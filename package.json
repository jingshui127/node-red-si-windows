{
  "name": "node-red-si-windows",
  "version": "1.0.0",
  "description": "Node-red portable windows",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "build64": "electron-packager . node-red-si --icon=./resources/nodered.ico --platform=win32 --arch=x64  --out=build --asar --overwrite --win32metadata.CompanyName='vitormnm' --win32metadata.ProductName='node-red-si'",
    "build32": "electron-packager . node-red-si --icon=./resources/nodered.ico --platform=win32 --arch=ia32  --out=build --asar --overwrite --win32metadata.CompanyName='vitormnm' --win32metadata.ProductName='node-red-si'"
  },
  "author": "vitormnm",
  "license": "MIT",
  "dependencies": {
    "@flowfuse/node-red-dashboard": "^1.3.0",
    "node-red": "^3.1.6",
    "node-red-contrib-aedes": "^0.11.1",
    "node-red-contrib-cip-ethernet-ip": "^1.1.3",
    "node-red-contrib-influxdb": "^0.7.0",
    "node-red-contrib-modbus": "^5.30.0",
    "node-red-contrib-mssql-plus": "^0.12.2",
    "node-red-contrib-nvl": "^1.1.1",
    "node-red-contrib-opcua": "^0.2.292",
    "node-red-contrib-pccc": "^1.0.2",
    "node-red-contrib-s7": "^3.1.0",
    "node-red-node-mongodb": "^0.2.5",
    "node-red-node-mysql": "^2.0.0",
    "node-red-node-ping": "^0.3.3",
    "node-red-node-sqlite": "^1.0.3"
  },
  "devDependencies": {
    "electron": "21.2.3",
    "electron-packager": "17.1.1"
  }
}
